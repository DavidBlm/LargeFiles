package ba.util;
//TODO: x has to be (n-1)n/2
//TODO: EMAM2Cpp does not work with matrix generics
component SingleSetCompare<N1 n = 2,N1 x = 1,T>{
    ports in T setIn[n],
          out T listOut1[x],
          out T listOut2[x];

    implementation Math{
        Z counter = 1;
        for i = 1:n
            for j = (i+1):n
                listOut1(counter) = setIn(i);
                listOut2(counter) = setIn(j);
                counter = counter + 1;
            end
        end
    }
}