package tests.dist;

component DistComp{
    component SubComp1{
        ports out Q out1,
              out Q out2;
    }

    component SubComp2{
        ports in Q in1,
              in Q in2;
    }

    instance SubComp1 sub1;
    instance SubComp2 sub2;

    connect sub1.out1 -> sub2.in1;
    connect sub1.out2 -> sub2.in2;
}