package cNNCalculator;

component Argmaxs{
        ports
                in Q(0:1)^10 input1,
                in Q(0:1)^10 input2,
                in Q(0:1)^10 input3,
                out Z(0:999) max;

        implementation Math{
            max = amax(input1) + 10*amax(input2) + 100*amax(input3);
        }
}
